{
  "name": "convenient-discussions",
  "version": "0.0.0",
  "description": "A JavaScript shell for wiki talk pages.",
  "scripts": {
    "build": "node buildConfigs.mjs && cross-env NODE_OPTIONS=--openssl-legacy-provider node buildI18n.mjs && cross-env NODE_OPTIONS=--openssl-legacy-provider webpack",
    "build16": "node buildConfigs.mjs && node buildI18n.mjs && webpack",
    "start": "node buildConfigs.mjs && cross-env NODE_OPTIONS=--openssl-legacy-provider node buildI18n.mjs && cross-env NODE_OPTIONS=--max-old-space-size=2048 NODE_OPTIONS=--openssl-legacy-provider webpack serve --env dev",
    "serve": "cross-env NODE_OPTIONS=--max-old-space-size=2048 NODE_OPTIONS=--openssl-legacy-provider webpack serve --env dev",
    "configs": "node buildConfigs.mjs",
    "deploy": "node --unhandled-rejections=strict deploy.mjs",
    "docs": "rimraf dist/docs && jsdoc --configure jsdoc/config.js --readme jsdoc/home.md",
    "i18n": "cross-env NODE_OPTIONS=--openssl-legacy-provider node buildI18n.mjs",
    "webpack": "cross-env NODE_OPTIONS=--openssl-legacy-provider webpack",
    "single": "node buildConfigs.mjs && cross-env NODE_OPTIONS=--openssl-legacy-provider node buildI18n.mjs && cross-env NODE_OPTIONS=--max-old-space-size=2048 NODE_OPTIONS=--openssl-legacy-provider webpack --env single",
    "test": "jest",
    "postinstall": "patch-package"
  },
  "browser": "src/app.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jwbth/convenient-discussions.git"
  },
  "author": "Jack who built the house",
  "license": "MIT",
  "bugs": {
    "url": "https://phabricator.wikimedia.org/tag/convenient-discussions/"
  },
  "homepage": "https://commons.wikimedia.org/wiki/User:Jack_who_built_the_house/Convenient_Discussions",
  "dependencies": {
    "date-fns": "^4.1.0",
    "date-fns-tz": "^3.2.0",
    "dayjs": "^1.11.18",
    "htmlparser2": "^10.0.0",
    "jest-environment-jsdom": "^30.1.2",
    "locutus": "^2.0.15",
    "lz-string": "^1.4.4",
    "oojs": "^7.0.1"
  },
  "devDependencies": {
    "@babel/core": "^7.28.0",
    "@babel/eslint-parser": "^7.28.0",
    "@babel/plugin-transform-class-properties": "^7.27.1",
    "@babel/plugin-transform-class-static-block": "^7.28.3",
    "@babel/plugin-transform-logical-assignment-operators": "^7.27.1",
    "@babel/plugin-transform-nullish-coalescing-operator": "^7.27.1",
    "@babel/plugin-transform-numeric-separator": "^7.18.6",
    "@babel/plugin-transform-optional-catch-binding": "^7.27.1",
    "@babel/plugin-transform-optional-chaining": "^7.27.1",
    "@babel/plugin-transform-runtime": "^7.28.0",
    "@babel/plugin-transform-typescript": "^7.28.0",
    "@babel/preset-env": "^7.28.0",
    "@babel/runtime": "^7.28.2",
    "@eslint/js": "^9.32.0",
    "@stylistic/eslint-plugin": "^5.4.0",
    "@types/jest": "^30.0.0",
    "@types/jquery": "^3.5.32",
    "@types/jqueryui": "^1.12.23",
    "@types/oojs": "^7.0.7",
    "@types/oojs-ui": "^0.49.4",
    "@types/prompts": "^2.4.9",
    "ajv": "^6.12.6",
    "babel-loader": "^8.2.2",
    "chalk": "^5.6.2",
    "codemirror": "^6.0.2",
    "cross-env": "^7.0.3",
    "css-loader": "^7.1.2",
    "docdash": "^1.2.0",
    "dompurify": "^3.2.6",
    "eslint": "^9.32.0",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-jsdoc": "^52.0.2",
    "eslint-plugin-no-one-time-vars": "^2.7.0",
    "eslint-plugin-unicorn": "^60.0.0",
    "http-proxy-to-socks": "^1.1.2",
    "jest": "^30.1.3",
    "jsdoc": "^3.6.6",
    "jsdom": "^27.0.0",
    "json5": "^2.2.3",
    "less": "^4.4.1",
    "less-loader": "^12.3.0",
    "node-fetch": "^3.2.10",
    "nodemw": "^0.24.1",
    "patch-package": "^8.0.0",
    "prompts": "^2.4.2",
    "react": "^16.14.0",
    "react-dom": "^16.14.0",
    "rimraf": "^6.0.1",
    "style-loader": "^4.0.0",
    "terser-webpack-plugin": "^5.3.10",
    "types-mediawiki": "^1.10.1",
    "typescript": "^5.9.2",
    "typescript-eslint": "^8.44.0",
    "webpack": "^5.101.3",
    "webpack-build-notifier": "^3.1.0",
    "webpack-cli": "^6.0.1",
    "webpack-dev-server": "^5.2.2",
    "windows-iana": "^5.0.2",
    "worker-loader": "^3.0.8",
    "yargs": "^18.0.0"
  },
  "sideEffects": [
    "./src/worker/worker.js",
    "./src/worker/extendDomhandler.js"
  ],
  "browserslist": [
    "> 1%"
  ],
  "engines": {
    "node": ">=16.0.0"
  }
}
